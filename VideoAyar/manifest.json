{
  "manifest_version": 2,
  "name": "Video Ses ve Hız Ayarlayıcı",
  "version": "1.1",
  "description": "Web sitelerinde videonun sesini ve hızını istediğiniz gibi ayarlayın.",
  "permissions": ["activeTab"],
  "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.svg"
  },
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
      }
  ],
  "icons": {
      "16": "icon.svg",
      "48": "icon.svg",
      "128": "icon.svg"
  }
}
